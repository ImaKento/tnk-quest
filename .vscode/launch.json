{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug React tnk_quest",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/tnk_quest_front",
            "preLaunchTask": "npm: start tnk_quest",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///./*": "${webRoot}/*"
            }
        }
    ]
}
